{
    "listeners": {
        "*:8000": {
            "pass": "routes",
            "forwarded": {
                "protocol": "X-Forwarded-Proto",
                "source": ["127.0.0.1"]
            }
        }
    },
    "routes": [
        {
            "match": {
                "uri": "/*"
            },
            "action": {
                "pass": "applications/laravel"
            }
        }
    ],
    "applications": {
        "laravel": {
            "type": "php",
            "user": "unit",
            "root": "/var/www/html/public",
            "index": "index.php"
        }
    }
}
